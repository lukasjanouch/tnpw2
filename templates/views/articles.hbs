    <script src="/js/script.js" defer></script>
</head>

<body>
    {{>header}}
    <!-- start banner Area -->
			<section class="banner-area">		
				<div class="container">
					<div class="row fullscreen align-items-center justify-content-between">
						<div class="col-lg-12 banner-content">
							<!--<h6 class="text-white">Wide Options of Choice</h6>-->
							<h1 class="text-white">F1 - Blog</h1>
							<p class="text-white">
								<!--inappropriate behavior is often laughed off as “boys will be boys,” women face higher conduct standards especially in the workplace. That’s why it’s crucial that, as women.-->
								Vytvoř si svůj článek o formuli 1 a sdílej ho s ostatními! Nebo můžeš jen tak pročítat, co napsali ostatní fanoušci. :)
							</p>
							<!--<a href="#" class="primary-btn text-uppercase">Check Our Menu</a>-->
						</div>
					</div>
				</div>					
			</section>
			<!-- End banner Area -->
    
    <section class="blog-area section-gap" id="blog">
				<div class="container">
                    	
                        <div class="row d-flex justify-content-center">
						<div class="menu-content pb-70 col-lg-8">
							<div class="title text-center">
								<h1 class="mb-10">{{>logged}}</h1>
                                <button class="primary-btn text-uppercase mt-20">Profil</button>
								
							</div>
						</div>
					</div>
        <div class="section-top-border">
						<div class="row">
						
                        <table>
        

    
     {{#each articles}}
        <div class="col-lg-3 col-md-6 col-sm-6 single-blog">
							<div class="thumb">
								<img class="img-fluid" src="img/b4.jpg" alt="">								
							</div>
							<p class="date">21 Mar 2020</p>
							<a href="blog-single.html"><h4>{{this.title}}</h4></a>
							<p>
								{{this.description}}
							</p>
							<div class="meta-bottom d-flex justify-content-between">
								<p><span class="lnr lnr-heart"></span> 00 Likes</p>
								<p><span class="lnr lnr-bubble"></span> 00 Comments</p>
							</div>									
						</div>
    {{/each}}
        
    </table>
					</div>
                    	</div>
					</div>
                    
    <!--  List of all tasks of user -->
    
    
    
    {{>footer}}
    
